{
    "name": "dleno/common-core",
    "type": "library",
    "description": "common core",
    "keywords": ["common-core"],
    "require": {
        "php": ">=8.1",
        "ext-swoole": ">=5.0",
        "ext-curl": "*",
        "hyperf/cache": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/db-connection": "~3.1.0",
        "hyperf/engine": "^2.10",
        "hyperf/framework": "~3.1.0",
        "hyperf/guzzle": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/logger": "~3.1.0",
        "hyperf/memory": "~3.1.0",
        "hyperf/process": "~3.1.0",
        "hyperf/laminas-mime": "^3.0",
        "hyperf/utils": "~3.1.0",
        "hyperf/redis": "~3.1.0",
        "hyperf/snowflake": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/paginator": "~3.1.0",
        "hyperf/validation": "~3.1.0",
        "hyperf/json-rpc": "~3.1.0",
        "hyperf/rpc-client": "~3.1.0",
        "hyperf/amqp": "~3.1.0",
        "hyperf/async-queue": "^3.1",
        "hyperf/signal": "~3.1.0",
        "hyperf/crontab": "~3.1.0",
        "dleno/hyperf-env-multi": "~3.1.0",
        "dleno/dingtalk": "~3.1.0",
        "symfony/serializer": "^5.0",
        "symfony/property-access": "^5.0",
        "hashids/hashids": "^4.1",
        "jenssegers/optimus": "^1.1"
    },
    "require-dev": {
        "swoole/ide-helper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Dleno\\CommonCore\\": "src/"
        },
        "files": [
            "src/common/Functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hyperf": {
            "config": "Dleno\\CommonCore\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
